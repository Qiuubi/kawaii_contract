{% extends 'base.html.twig' %}

{% block title %}Amendement
{% endblock %}

{% block body %}
	<h1>Amendement</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ amendement.id }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ amendement.name }}</td>
			</tr>
			<tr>
				<th>Description</th>
				<td>{{ amendement.description }}</td>
			</tr>
			<tr>
				<th>Term</th>
				<td>{{ amendement.term }}</td>
			</tr>
			<tr>
				<th>DateEffect</th>
				<td>{{ amendement.dateEffect ? amendement.dateEffect|date('Y-m-d') : '' }}</td>
			</tr>
			<tr>
				<th>DateEnding</th>
				<td>{{ amendement.dateEnding ? amendement.dateEnding|date('Y-m-d') : '' }}</td>
			</tr>
			<tr>
				<th>CreatedAt</th>
				<td>{{ amendement.createdAt ? amendement.createdAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
			<tr>
				<th>NewProvisions</th>
				<td>{{ amendement.newProvisions }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('amendement_edit', {'id': amendement.id}) }}">edit</a>

	{#{ include('amendement/_delete_form.html.twig') }#}
{% endblock %}
