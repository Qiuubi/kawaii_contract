<section id="kawaii-contract-soon-finish" class="kawaii-contract-part home">
	<h1 class="kawaii-contract-main-title soon-finish">Contrats arrivant à leur terme</h1>
	{% for contractSoonFinish in contractsSoonFinish %}
		{% set dateEff = contractSoonFinish.dateEffect|date('d-m-Y') %}
		{% set dateEnd = contractSoonFinish.dateEnding|date('d-m-Y') %}
		{% set today = "now"|date('d-m-Y') %}
		{% set totalDays = date(dateEnd).diff(date(dateEff))%}
		{% set dateEffectToday = date(today).diff(date(dateEff))%}
		{% set percent = dateEffectToday.days  * 100 / totalDays.days %}
		{% set daysbetweenTodayDateEnding = date(dateEnd).diff(date(today))%}
		{% set contractId = contractSoonFinish.id %}
		{% if daysbetweenTodayDateEnding.days < 165 %}
			<article class="kawaii-contract-first-block soon-finish">
